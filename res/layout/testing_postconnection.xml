            android:baselineAligned="false"
            android:background="@drawable/cell_background"
            android:paddingBottom="2dp" >
            <TextView 
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:text="RCD Operating Times (ms)"
                android:textSize="15sp"
                android:textStyle="bold"
                android:background="@drawable/cell_background"
