                android:background="@drawable/cell_background"
                android:gravity="center"/>
            <com.powerblock.timesheets.CustomEditText
                android:id="@+id/post_connection_value_4"
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:textSize="15sp"
                android:background="@drawable/cell_background"
                android:gravity="center"
                android:hint="Enter Text" />
            <com.powerblock.timesheets.CustomEditText
                android:id="@+id/post_connection_value_5"
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:textSize="15sp"
                android:background="@drawable/cell_background"
                android:gravity="center"
                android:hint="Enter Text" />
            <com.powerblock.timesheets.CustomEditText
