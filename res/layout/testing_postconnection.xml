            <com.powerblock.timesheets.CustomCheckBox
                android:id="@+id/post_connection_value_10"
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:background="@drawable/cell_background"/>
            <com.powerblock.timesheets.CustomCheckBox
                android:id="@+id/post_connection_value_11"
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:background="@drawable/cell_background"/>
            <com.powerblock.timesheets.CustomCheckBox
                android:id="@+id/post_connection_value_12"
                android:layout_width="@dimen/connection_cell_width"
                android:layout_height="@dimen/connection_cell_height"
                android:background="@drawable/cell_background"/>
        </TableRow>
	</TableLayout>
</RelativeLayout>